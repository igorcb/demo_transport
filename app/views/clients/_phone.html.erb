<fieldset class="item">
  <%= f.label :place %>
  <%= f.select :place, ['Home', 'Work', 'Other']  %>

  <%= f.label :number %>
  <%= f.text_field :number, size: '20' %>

  <a href="#" class="remove" data-confirm="Are you sure?">remove</a>

  <%= f.hidden_field :id %>
  <%= f.hidden_field :_destroy %>
</fieldset>
